<p id="notice"><%= notice %></p>

<h1>Age Groups</h1>

<table>
  <thead>
    <tr>
      <th>Personal information</th>
      <th>Age</th>
      <th>Age point</th>
      <th>Age adjustment</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @age_groups.each do |age_group| %>
      <tr>
        <td><%= age_group.personal_information_id %></td>
        <td><%= age_group.age %></td>
        <td><%= age_group.age_point %></td>
        <td><%= age_group.age_adjustment %></td>
        <td><%= age_group.date %></td>
        <td><%= link_to 'Show', age_group %></td>
        <td><%= link_to 'Edit', edit_age_group_path(age_group) %></td>
        <td><%= link_to 'Destroy', age_group, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Age Group', new_age_group_path %>
