<p id="notice"><%= notice %></p>

<h1>User Yongs</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_yongs.each do |user_yong| %>
      <tr>
        <td><%= user_yong.name %></td>
        <td><%= user_yong.age %></td>
        <td><%= link_to 'Show', user_yong %></td>
        <td><%= link_to 'Edit', edit_user_yong_path(user_yong) %></td>
        <td><%= link_to 'Destroy', user_yong, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Yong', new_user_yong_path %>
