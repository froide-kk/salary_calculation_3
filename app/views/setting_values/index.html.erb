<p id="notice"><%= notice %></p>

<h1>Setting Values</h1>

<table>
  <thead>
    <tr>
      <th>Setting list</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @setting_values.each do |setting_value| %>
      <tr>
        <td><%= setting_value.setting_list %></td>
        <td><%= setting_value.value %></td>
        <td><%= link_to 'Show', setting_value %></td>
        <td><%= link_to 'Edit', edit_setting_value_path(setting_value) %></td>
        <td><%= link_to 'Destroy', setting_value, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Setting Value', new_setting_value_path %>
